<div class="container text-center">
<h1>Sign Up Here</h1>
	<div class="row">
	<%= form_for @store_owner, url: signup_path do |u| %>
		<%= label_tag 'Name' %>
		<%= u.text_field :name %>
		<%= label_tag 'Email' %>
		<%= u.text_field :email %>
		<%= label_tag 'Password' %>
		<%= u.password_field :password %>
		<%= u.submit "Sign Up" %>
	<% end %>
	<div>
</div>